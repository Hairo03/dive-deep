@model IReadOnlyList<BookingItem>

@if (Model.Any())
{
    <ul class="list-group mb-3">
        @foreach (var item in Model)
        {
            <li class="list-group-item d-flex justify-content-between align-items-center">
                <span>@item.Name</span>
                <span>@item.TotalPrice.ToString("0.00") kr.</span>
            </li>
        }
    </ul>
}
else
{
    <p>Kurven er tom.</p>
}